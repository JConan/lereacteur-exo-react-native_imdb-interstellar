{
  "expo": {
    "owner": "jconan",
    "name": "imdb-interstellar",
    "githubUrl": "https://github.com/JConan/lereacteur-exo-react-native_imdb-interstellar.git",
    "slug": "imdb-interstellar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jconan.imdbinterstellar"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.jconan.imdbinterstellar"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
